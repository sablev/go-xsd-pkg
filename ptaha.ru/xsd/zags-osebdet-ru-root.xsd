<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="urn://x-artefacts-zags-osebdet/root/112-57/4.0.2" xmlns:fnst="urn://x-artefacts-zags-osebdet/types/4.0.2" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:usch="http://www.unisoftware.ru/schematron-extensions" xmlns:ns1="urn://x-artefacts-zags-osebdet/root/112-57/4.0.2" targetNamespace="urn://x-artefacts-zags-osebdet/root/112-57/4.0.2" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="urn://x-artefacts-zags-osebdet/types/4.0.2" schemaLocation="./commons/zags-osebdet-types.xsd"/>
	<xs:element name="OSEBDETRequest">
		<xs:annotation>
			<!---************************************************************************************************************************************************************-->
			<xs:appinfo>
				<sch:pattern name="Проверка присутствия (отсутствия) элемента МуниципРайон в зависимости от значения элемента Регион">
					<sch:rule context="*[local-name()='АдрМО']">
						<sch:assert test="usch:iif(*[local-name()='Регион']!=99, count(*[local-name()='МуниципРайон'])!=0, count(*[local-name()='МуниципРайон'])!=0 or count(*[local-name()='МуниципРайон'])=0)">
							<usch:error code="0400300003" path="count(МуниципРайон)" value-of-select="*[local-name()='Регион']">Не выполнено условие присутствия (отсутствия) элемента МуниципРайон при значении элемента Регион, равном "<usch:value-of select="*[local-name()='Регион']"/>" </usch:error>
						</sch:assert>
					</sch:rule>
				</sch:pattern>
			</xs:appinfo>
			<!---*************************************************************************************************************************************************************-->
			<xs:documentation>Запрос на предоставление сведений, содержащихся в ЕГР ЗАГС, составленных в отношении лица или каждого из его детей, не достигших совершеннолетия, в рамках работы сервиса на ЕПГУ</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="СведЗапр">
					<xs:annotation>
						<xs:documentation>Сведения, содержащиеся в запросе</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="СвРебЗапр" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Сведения о ребенке, в отношении которого осуществлен запрос</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ФИО" type="fnst:ФИОПрТип">
											<xs:annotation>
												<xs:documentation>Фамилия, имя, отчество</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:choice>
											<xs:element name="ДатаРожд" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата рождения</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="МесГодРожд" type="xs:gYearMonth">
												<xs:annotation>
													<xs:documentation>Месяц и год рождения</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="ГодРожд" type="xs:gYear">
												<xs:annotation>
													<xs:documentation>Год рождения</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:choice>
										<xs:element name="УдЛичнФЛ" type="fnst:УдЛичнФЛТип" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Сведения о документе, удостоверяющем личность </xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="СведСвидетРожд" type="fnst:СведСвидетБезТип">
											<xs:annotation>
												<xs:documentation>Сведения Свидетельства о рождении лица, в отношении которого осуществлен запрос</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="СведСвидетОтцов" type="fnst:СведСвидетБезТип" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Сведения о Свидетельстве об установлении отцовства</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="СведАГС" type="fnst:СведАГСТип" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Сведения о государственной регистрации АГС, в отношении которого сформирован запрос</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:choice>
											<xs:element name="ПрМестоРожд">
												<xs:annotation>
													<xs:documentation>Признак отсутствия сведений о месте рождения</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="fnst:string-1">
														<xs:enumeration value="1"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="МестоРожд" type="fnst:string-1024">
												<xs:annotation>
													<xs:documentation>Место рождения</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:choice>
									</xs:sequence>
									<xs:attribute name="СНИЛС" type="fnst:СНИЛСТип" use="optional">
										<xs:annotation>
											<xs:documentation>СНИЛС</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="СвЛицЗаяв" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Сведения о лице, направившем запрос</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ФИО" type="fnst:ФИОПрТип">
											<xs:annotation>
												<xs:documentation>Фамилия, имя, отчество</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:choice>
											<xs:element name="ДатаРожд" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата рождения</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="МесГодРожд" type="xs:gYearMonth">
												<xs:annotation>
													<xs:documentation>Месяц и год рождения</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="ГодРожд" type="xs:gYear">
												<xs:annotation>
													<xs:documentation>Год рождения</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:choice>
										<xs:element name="УдЛичнФЛ" type="fnst:УдЛичнФЛТип">
											<xs:annotation>
												<xs:documentation>Сведения о документе, удостоверяющем личность </xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:choice>
											<xs:element name="ПрАдрМЖ">
												<xs:annotation>
													<xs:documentation>Признак отсутствия сведений об адресе места жительства</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="fnst:string-1">
														<xs:enumeration value="1"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="АдрМЖ" type="fnst:АдрМЖТип">
												<xs:annotation>
													<xs:documentation>Адрес места жительства</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:choice>
										<xs:element name="СведАГС" type="fnst:СведАГСТип" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Сведения о государственной регистрации АГС, в отношении которого сформирован запрос</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:choice>
											<xs:element name="ПрМестоРожд">
												<xs:annotation>
													<xs:documentation>Признак отсутствия сведений о месте рождения</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="fnst:string-1">
														<xs:enumeration value="1"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="МестоРожд" type="fnst:string-1024">
												<xs:annotation>
													<xs:documentation>Место рождения</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:choice>
									</xs:sequence>
									<xs:attribute name="ТипСвед" use="required">
										<xs:annotation>
											<xs:documentation>Тип передаваемых сведений</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="fnst:string-1">
												<xs:enumeration value="1"/>
												<xs:enumeration value="2"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="СНИЛС" type="fnst:СНИЛСТип" use="required">
										<xs:annotation>
											<xs:documentation>СНИЛС</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="ПрСвОтнош" use="required">
							<xs:annotation>
								<xs:documentation>Признак, в отношении кого запрашивают сведения</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="fnst:string-1">
									<xs:enumeration value="1"/>
									<xs:enumeration value="2"/>
									<xs:enumeration value="3"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="ПерЗапСв" use="required">
							<xs:annotation>
								<xs:documentation>Период запрашиваемых сведений</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="fnst:string-1">
									<xs:enumeration value="1"/>
									<xs:enumeration value="2"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="ИдЗапрос" type="fnst:ИдЗапросТип" use="required">
				<xs:annotation>
					<xs:documentation>Идентификатор запроса, сформированный запрашивающей стороной</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ИдЕСИА" type="fnst:string-36" use="required">
				<xs:annotation>
					<xs:documentation>Идентификатор ЕСИА</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ДатаЗапр" type="xs:date" use="required">
				<xs:annotation>
					<xs:documentation>Дата формирования запроса</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ТипЗапрос" use="required">
				<xs:annotation>
					<xs:documentation>Тип запроса</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="fnst:string-1">
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
						<xs:enumeration value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="OSEBDETResponse">
		<xs:annotation>
			<xs:documentation>Ответ на запрос, содержащий сведения, найденные в ЕГР ЗАГС о запрашиваемом лице или каждом из его детей, не достигших совершеннолетия, в рамках работы сервиса на ЕПГУ</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="СведЕГР" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Сведения, найденные в ЕГР ЗАГС о запрашиваемом лице или каждом из его детей, не достигших совершеннолетия, в рамках работы сервиса на ЕПГУ</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="СвЛицЗапр">
								<xs:annotation>
									<xs:documentation>Сведения о лице, в отношении которого осуществлен запрос</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ФИО" type="fnst:ФИОПрТип">
											<xs:annotation>
												<xs:documentation>Фамилия, имя, отчество родителя</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:choice>
											<xs:element name="ДатаРожд" type="xs:date">
												<xs:annotation>
													<xs:documentation>Дата рождения</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="МесГодРожд" type="xs:gYearMonth">
												<xs:annotation>
													<xs:documentation>Месяц и год рождения</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="ГодРожд" type="xs:gYear">
												<xs:annotation>
													<xs:documentation>Год рождения</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:choice>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:choice>
								<xs:element name="ПрУвед">
									<xs:annotation>
										<xs:documentation>Признак отсутствия уведомления о прекращении, отклонении, изменении</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="fnst:string-1">
											<xs:enumeration value="1"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="УведПрек">
									<xs:annotation>
										<xs:documentation>Уведомление о прекращении предоставления сведений о государственной регистрации актов гражданского состояния</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="fnst:string-1">
											<xs:enumeration value="1"/>
											<xs:enumeration value="2"/>
											<xs:enumeration value="3"/>
											<xs:enumeration value="4"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="УведОткл" type="fnst:string-500">
									<xs:annotation>
										<xs:documentation>Уведомление об отклонении запроса на предоставление сведений о государственной регистрации актов гражданского состояния</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="УведИзм" type="fnst:string-500">
									<xs:annotation>
										<xs:documentation>Уведомление об изменении критериев запроса сведений</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="СвАкт">
									<xs:annotation>
										<xs:documentation>Сведения об актах гражданского состояния</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="СведРегРожд" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Сведения о государственной регистрации рождения</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:choice>
															<xs:element name="ДатаЗаписДок" type="fnst:ДатаДокТип">
																<xs:annotation>
																	<xs:documentation>Дата регистрации записи по представленным документам (неполная дата)</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="ДатаЗапис" type="xs:date">
																<xs:annotation>
																	<xs:documentation>Дата составления записи акта о рождении</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:choice>
														<xs:element name="ОрганЗАГС" type="fnst:ОрганЗАГСТип">
															<xs:annotation>
																<xs:documentation>Сведения об органе ЗАГС, которым произведена государственная регистрация рождения ребенка</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="СостСтатЗаписи" type="fnst:СостСтатЗапТип">
															<xs:annotation>
																<xs:documentation>Сведения о состоянии и статусе записи акта о рождении</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="СвидетРожд" type="fnst:СведСвидетТип" minOccurs="0" maxOccurs="unbounded">
															<xs:annotation>
																<xs:documentation>Сведения о выданном Свидетельстве о рождении</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="ПрдСведРегРожд">
															<xs:annotation>
																<xs:documentation>Передаваемые сведения о государственной регистрации рождения</xs:documentation>
															</xs:annotation>
															<xs:complexType>
																<xs:sequence>
																	<xs:element name="СведСчетРебен" minOccurs="0">
																		<xs:annotation>
																			<xs:documentation>Сведения о том, которым по счету ребенок был рожден у матери</xs:documentation>
																		</xs:annotation>
																		<xs:complexType>
																			<xs:choice>
																				<xs:element name="ПрСчетРебен">
																					<xs:annotation>
																						<xs:documentation>Признак отсутствия сведений о том, которым по счету ребенок был рожден у матери</xs:documentation>
																					</xs:annotation>
																					<xs:simpleType>
																						<xs:restriction base="fnst:string-1">
																							<xs:enumeration value="1"/>
																						</xs:restriction>
																					</xs:simpleType>
																				</xs:element>
																				<xs:element name="СчетРебен" type="fnst:number-2">
																					<xs:annotation>
																						<xs:documentation>Которым по счету ребенок был рожден у матери</xs:documentation>
																					</xs:annotation>
																				</xs:element>
																			</xs:choice>
																		</xs:complexType>
																	</xs:element>
																	<xs:element name="СведРоды" minOccurs="0">
																		<xs:annotation>
																			<xs:documentation>Сведения о родах, в результате которых родился ребенок</xs:documentation>
																		</xs:annotation>
																		<xs:complexType>
																			<xs:sequence>
																				<xs:choice>
																					<xs:element name="ПрПризнакРодов">
																						<xs:annotation>
																							<xs:documentation>Признак отсутствия сведений о признаке родов</xs:documentation>
																						</xs:annotation>
																						<xs:simpleType>
																							<xs:restriction base="fnst:string-1">
																								<xs:enumeration value="1"/>
																							</xs:restriction>
																						</xs:simpleType>
																					</xs:element>
																					<xs:element name="ПризнакРодов">
																						<xs:annotation>
																							<xs:documentation>Признак родов, при которых родился ребенок</xs:documentation>
																						</xs:annotation>
																						<xs:simpleType>
																							<xs:restriction base="fnst:string-1">
																								<xs:enumeration value="1"/>
																								<xs:enumeration value="2"/>
																							</xs:restriction>
																						</xs:simpleType>
																					</xs:element>
																				</xs:choice>
																				<xs:element name="СведМногоплРоды" minOccurs="0">
																					<xs:annotation>
																						<xs:documentation>Сведения о многоплодных родах</xs:documentation>
																					</xs:annotation>
																					<xs:complexType>
																						<xs:sequence>
																							<xs:choice>
																								<xs:element name="ПрЧислоРодДетей">
																									<xs:annotation>
																										<xs:documentation>Признак отсутствия сведений о числе родившихся детей при многоплодных родах</xs:documentation>
																									</xs:annotation>
																									<xs:simpleType>
																										<xs:restriction base="fnst:string-1">
																											<xs:enumeration value="1"/>
																										</xs:restriction>
																									</xs:simpleType>
																								</xs:element>
																								<xs:element name="ЧислоРодДетей" type="fnst:number-2">
																									<xs:annotation>
																										<xs:documentation>Число родившихся детей при многоплодных родах</xs:documentation>
																									</xs:annotation>
																								</xs:element>
																							</xs:choice>
																							<xs:choice>
